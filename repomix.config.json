{
  "output": {
    "instructionFilePath": ".github/repomix-instruction.md",
    "filePath": "new-llm.xml",
    "style": "xml",
    "parsableStyle": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "compress": false,
    "topFilesLength": 20,
    "showLineNumbers": false,
    "copyToClipboard": false,
    "git": {
      "sortByChanges": false
    }
  },
  "include": ["**/*"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      ".github/repomix-instruction.md",
      "src/prompts/sections.ts",
      "content/",
      "docs",
      "test/",
      "whisper.cpp",
      ".env",
      ".gitignore",
      "LICENSE",
      "new-llm.md",
      "new-*.md",
      "new*.md",
      "package-lock.json",
      "repomix.config.json",
      "tsconfig.json"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}